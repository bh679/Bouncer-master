blocktype = argument0

if blocktype = "wood"
    Levels.firstWood = Levels.roomindex
else if blocktype = "sand"
    Levels.firstSand = Levels.roomindex
else if blocktype = "lava"
    Levels.firstLava = Levels.roomindex
else if blocktype = "hotair"
    Levels.firstHotair = Levels.roomindex
else if blocktype = "box"
    Levels.firstBox = Levels.roomindex
else if blocktype = "tramp"
    Levels.firstTramp = Levels.roomindex

